<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	         xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
	</ui:style>

	<g:HTMLPanel addStyleNames="panel panel-default">
		<div ui:field="header" class="panel-heading">
			<h3 ui:field="headerText" class="panel-title"></h3>
		</div>
		<g:FlowPanel ui:field="content" addStyleNames="panel-body">
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder>